<?xml version="1.0" encoding="utf-8"?>
<model>
		<state label="initial">
			<name>On Hook</name>
			<arc label= "prob">
			    	<name>incomming call</name>
					<stimulate>
							<parameter>
							 	<paramName>x</paramName>
								<paramType>double</paramType>
								<domain type="serial">1.0&#8804;x&#8804;6.0</domain>
							</parameter>
							<parameter>
							 	<paramName>y</paramName>
								<paramType>double</paramType>
								<domain type="serial">3.0&lt;y&#8804;9.0</domain>
							</parameter>
								<constraint>6.0&#8804;x+y&lt;13.0</constraint>
								<constraint>-2.0&#8804;x-y&lt;3.0</constraint>
					</stimulate>
					<to>Ringing</to>
					<prob>0.5</prob>
			</arc>
			<arc label= "prob">
					<name>lift receiver</name>
					<stimulate>
							<parameter>
							 	<paramName>a</paramName>
								<paramType>double</paramType>
								<domain type="serial">2.0&#8804;a&#8804;6.0</domain>
							</parameter>
							<parameter>
							 	<paramName>route</paramName>
								<paramType>string</paramType>
								<domain type="discrete">straight,curve</domain>
							</parameter>
					</stimulate>
					<to>Off Hook</to>
					<prob>0.5</prob>
			</arc>
		</state>
		<state>
			<name>Ringing</name>
				<arc label= "prob">
					<name>lift receiver</name>
					<stimulate>
							<parameter>
							 	<paramName>x</paramName>
								<paramType>int</paramType>
								<domain type="serial">0&#8804;x&#8804;6</domain>
							</parameter>
					</stimulate>
					<to>Connected</to>
					<prob>1.0</prob>
			</arc>
			<!-- <arc label= "prob">						
				    <name>disconnect</name>
					<stimulate>
							<parameter>
							 	<paramName>mode</paramName>
								<paramType>string</paramType>
								<domain type="discrete">A,B,C</domain>
							</parameter>
							<parameter>
							 	<paramName>height</paramName>
								<paramType>string</paramType>
								<domain type="discrete">G,D</domain>
							</parameter>
					</stimulate>
					<to>On Hook</to>
					<prob>0.5</prob>
			</arc> -->
		</state>
		<state>
			<name>Off Hook</name>
			<arc label= "prob">
					<name>dial bad</name>
					<stimulate>
							<parameter>
							 	<paramName>m</paramName>
								<paramType>int</paramType>
								<domain type="serial">1&#8804;m&#8804;6</domain>
							</parameter>
							<parameter>
							 	<paramName>speed</paramName>
								<paramType>string</paramType>
								<domain type="discrete">F,M,S</domain>
							</parameter>
							<parameter>
							 	<paramName>n</paramName>
								<paramType>int</paramType>
								<domain type="serial">3&lt;n&#8804;9</domain>
							</parameter>
								<constraint>6&#8804;m+n&lt;13</constraint>
								<constraint>-2&#8804;m-n&lt;3</constraint>
					</stimulate>
					<to>Error Tone</to>
					<prob>0.25</prob>
				</arc>
				<arc label= "prob">
					<name>dial busy</name>
					<to>Busy Tone</to>
					<prob>0.25</prob>
				</arc>
				<arc label= "prob">
					<name>dial good</name>
					<stimulate>
							<parameter>
							 	<paramName>b</paramName>
								<paramType>double</paramType>
								<domain type="serial">b&gt;1.0</domain>
							</parameter>
					</stimulate>
					<to>Ring Tone</to>
					<prob>0.25</prob>
				</arc>
				<arc label= "prob">
					<name>hang up</name>
					<stimulate>
							<parameter>
							 	<paramName>c</paramName>
								<paramType>int</paramType>
								<domain type="discrete">3,6,7</domain>
							</parameter>
					</stimulate>
					<to>Exit</to>
					<prob>0.25</prob>
				</arc>
		</state>
		<state>
			<name>Connected</name>
			<!-- <arc label= "prob">
					<name>disconnect</name>
					<to>Off Hook</to>
					<prob>0.5</prob>
				</arc> -->
				<arc label= "prob">
					<name>hang up</name>
					<stimulate>
							<parameter>
							 	<paramName>d</paramName>
								<paramType>double</paramType>
								<domain type="serial">2.0&#8804;d&#8804;6.0</domain>
							</parameter>
							<parameter>
							 	<paramName>e</paramName>
								<paramType>int</paramType>
								<domain type="serial">3&#8804;e&#8804;10</domain>
							</parameter>
								<constraint>12&#8804;d*e&lt;49</constraint>
					</stimulate>
					<to>Exit</to>
					<prob>1.0</prob>
			</arc>
		</state>
		<state>
			<name>Busy Tone</name>
			<arc label= "prob">
					<name>hang up</name>
					<stimulate>
							<parameter>
							 	<paramName>f</paramName>
								<paramType>double</paramType>
								<domain type="serial">8.0&#8804;f&#8804;16.0</domain>
							</parameter>
							<parameter>
							 	<paramName>g</paramName>
								<paramType>int</paramType>
								<domain type="discrete">6,8,12</domain>
							</parameter>
					</stimulate>
					<to>Exit</to>
					<prob>1.0</prob>
			</arc>
		</state>
		<state>
			<name>Error Tone</name>
			<arc label= "prob">
					<name>hang up</name>
					<stimulate>
							<parameter>
							 	<paramName>i</paramName>
								<paramType>int</paramType>
								<domain type="discrete">66,77,88</domain>
							</parameter>
							<parameter>
							 	<paramName>j</paramName>
								<paramType>double</paramType>
								<domain type="discrete">8.8,10.6,11.8</domain>
							</parameter>
					</stimulate>
					<to>Exit</to>
					<prob>1.0</prob>
			</arc>
		</state>
		<state>
			<name>Ring Tone</name>
			<arc label= "prob">
					<name>hang up</name>
					<stimulate>
							<parameter>
							 	<paramName>o</paramName>
								<paramType>double</paramType>
								<domain type="serial">3.0&#8804;o&#8804;11.8</domain>
							</parameter>
							<parameter>
							 	<paramName>pattern</paramName>
								<paramType>string</paramType>
								<domain type="discrete">P1,P2,P3</domain>
							</parameter>
							<parameter>
							 	<paramName>q</paramName>
								<paramType>int</paramType>
								<domain type="serial">6&#8804;q&#8804;20</domain>
							</parameter>
								<constraint>o+q&lt;16.0</constraint>
					</stimulate>
					<to>Exit</to>
					<prob>0.5</prob>
			</arc>
			<arc label= "prob">
					<name>connect</name>
					<to>Connected</to>
					<prob>0.5</prob>
			</arc>
		</state>
		<state label="final">
			<name>Exit</name>
		</state>
</model>
